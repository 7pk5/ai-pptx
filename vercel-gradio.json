{
  "version": 2,
  "builds": [
    {
      "src": "api/gradio_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/gradio_app.py"
    }
  ],
  "functions": {
    "api/gradio_app.py": {
      "maxDuration": 60
    }
  },
  "env": {
    "PYTHONPATH": "."
  }
}
